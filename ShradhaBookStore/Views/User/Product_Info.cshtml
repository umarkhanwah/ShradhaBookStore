@model ShradhaBookStore.Models.Allproduct

@{
    Layout = "_LayoutGuest";
    ViewData["Title"] = "Product Information";
    dynamic heading = ViewData["Heading"];
    var id = heading.id;
    var name = heading.Name;
}


<!-- Open Content -->
<section class="bg-light">
    <div class="container pb-5">
        @if (@ViewData["msg"] != null)
        {
            <div class="alert alert-info">
                @ViewData["msg"]
            </div>

        }
        @if (Model.Products.Any())
        {
            var product = Model.Products.First();

            <div class="row">
                <div class="col-lg-5 mt-5 h-100">
                    <div class="card mb-3">
                        <img class="card-img img-fluid" src="~/Images/Products/@product.Image" alt="@product.Name" id="product-detail">
                    </div>

               
                </div>
                <!-- col end -->
                <div class="col-lg-7 mt-5 h-100">
                    <div class="card">
                        <div class="card-body">
                            <h1 class="h2">@product.Name</h1>
                            <p class="h3 py-2">Rs : @product.Price</p>
                            <p class="py-2">
                                <i class="fa fa-star text-warning"></i>
                                <i class="fa fa-star text-warning"></i>
                                <i class="fa fa-star text-warning"></i>
                                <i class="fa fa-star text-warning"></i>
                                <i class="fa fa-star text-secondary"></i>
                                <span class="list-inline-item text-dark">Rating 4.8 | 36 Comments</span>
                            </p>
                            <ul class="list-inline">
                                <li class="list-inline-item">
                                    <h6>Manufacturer:</h6>
                                </li>
                                <li class="list-inline-item">
                                    <p class="text-muted"><strong>@product.Manufacturer.Name</strong></p>
                                </li>
                            </ul>

                            <h6>Description:</h6>
                            <p>@product.Description</p>
                            <ul class="list-inline">
                                <li class="list-inline-item">
                                    <h6>Acronyms :</h6>
                                </li>
                                <li class="list-inline-item">
                                    <p class="text-muted"><strong>@product.Acronym</strong></p>
                                </li>
                            </ul>

                         

                            
                                <input type="hidden" name="product-title" value="Activewear">
                                <div class="row">
                                    <div class="col-auto">
                                        <ul class="list-inline pb-3">
                                            <li class="list-inline-item">
                                                Size :
                                                <input type="hidden" name="product-size" id="product-size" value="S">
                                            </li>
                                            <li class="list-inline-item"><span class="btn btn-success btn-size">S</span></li>
                                            <li class="list-inline-item"><span class="btn btn-success btn-size">M</span></li>
                                            <li class="list-inline-item"><span class="btn btn-success btn-size">L</span></li>
                                            <li class="list-inline-item"><span class="btn btn-success btn-size">XL</span></li>
                                        </ul>
                                    </div>
                                    <div class="col-auto">
                                        <ul class="list-inline pb-3">
                                            <li class="list-inline-item text-right">
                                                Quantity
                                            <input type="hidden" id="product-quantity"  value="1">
                                            </li>
                                            <li class="list-inline-item" ><span onclick="updateQuantity()" class="btn btn-success" id="btn-minus" >-</span></li>
                                            <li class="list-inline-item"><span class="badge bg-secondary" id="var-value" >1</span></li>
                                            <li class="list-inline-item"><span onclick="updateQuantity()" class="btn btn-success" id="btn-plus" >+</span></li>
                                        </ul>
                                    </div>
                                </div>
                                <div class="row pb-3">
                                    <div class="col d-grid">
                                        @if (ViewData["Wishlist"] == null)
                                        {
                                            <a asp-action="Add_to_Wishlist" asp-route-product="@ViewData["Wishlist"]" class="btn btn-outline-danger btn-lg">Add To Wishlist</a>
                                        }
                                        else
                                        {
                                            <a asp-action="Remove_Wishlist" asp-route-wishid="@ViewData["Wishlist"]" class="btn btn-danger btn-lg">Remove</a>
                                        }
                                    </div>
                                    <div class="col d-grid">
                                        @if(ViewData["Cart"] == null){

                                        <a asp-action="Add_to_Cart" id="addToCartBtn" asp-route-product="@product.Id" asp-route-price="@product.Price" asp-route-quantity="1" class="btn btn-outline-primary btn-lg w-100">Add To Cart</a>
                                        }
                                        else
                                        {
                                            <a asp-action="Show_Cart"  class="btn btn-primary btn-lg w-100">View Cart</a>
                                        
                                        }
                                </div>
                                  
                                </div>

                        </div>
                    </div>
                </div>
            </div>
        }
    </div>
</section>
<!-- Close Content -->
